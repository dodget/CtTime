

<% if current_user.admin? %>

  <div class="container">

    <div class="row">
      <h1>Import the schedule</h1>
    </div><!--row-->

    <div class="row">
      <%= form_tag import_schedules_path, multipart: true do %>
        <label for="file">File to Upload</label> <%= file_field_tag :file %>
        <%= submit_tag "Import"%>

    </div><!--row-->

  </div><!--container-->
  <% end %>

<% else %>

  <h3>This is restricted to administrators.</h3>


<% end %>
