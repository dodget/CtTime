<p id="notice"><%= notice %></p>

<h1>Listing Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Course name</th>
      <th>Class name</th>
      <th>Specialty</th>
      <th>Start</th>
      <th>End</th>
      <th>Location</th>
      <th>Hours</th>
      <th>In hst</th>
      <th>Ct assigned</th>
      <th>Backup ct</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.course_name %></td>
        <td><%= schedule.class_name %></td>
        <td><%= schedule.specialty %></td>
        <td><%= schedule.start %></td>
        <td><%= schedule.ending %></td>
        <td><%= schedule.location %></td>
        <td><%= schedule.hours %></td>
        <td><%= schedule.in_hst %></td>
        <td><%= schedule.ct_assigned %></td>
        <td><%= schedule.backup_ct %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
